#include<iostream>
using namespace std;
#include<string.h>
int main()
{
    int i;
    int count=1;
    string  s;
    while(cin>>s)
    {
        if(s=="*")
        {
            break;
        }
        if(s== "Hajj")
        {
            cout<<"Case "<<count ++<<": Hajj-e-Akbar"<<"\n";
        }
        if(s== "Umrah")
        {
            cout<<"Case "<<count++<<": Hajj-e-Asghar"<<"\n";
        }


    }
    return 0;
}

