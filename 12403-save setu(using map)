#include<bits/stdc++.h>
#include<iostream>
using namespace std;
int main()
{
    long long int i,n=0,t,sum=0;
    map<int,int>mp;
    string s;
    cin>>t;
    for(i=0; i<t; i++)
    {
        cin>>s;
        if(s=="donate")
        {
            ++n;
            cin>>mp[n];
            sum=sum+mp[n];
        }
        else
        {
            cout<<sum<<endl;
        }
    }



    return 0;
}


